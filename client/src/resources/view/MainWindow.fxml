<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="679.0" prefWidth="1100.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.MainWindowController">
    <VBox alignment="TOP_CENTER" prefHeight="680.0" prefWidth="1100.0" AnchorPane.bottomAnchor="0.0"
          AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <TabPane prefHeight="600.0" prefWidth="900.0" tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
            <Tab fx:id="tableTab" text="TableTab">
                <AnchorPane prefHeight="180.0" prefWidth="200.0">
                    <TableView fx:id="labWorkTable" prefHeight="542.0" prefWidth="1100.0"
                               AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                               AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                        <columns>
                            <TableColumn fx:id="idColumn" prefWidth="19.0" text="ID" />
                            <TableColumn fx:id="userColumn" prefWidth="64.0" text="User" />
                            <TableColumn fx:id="nameColumn" prefWidth="119.0" text="Name" />
                            <TableColumn fx:id="creationDateColumn" prefWidth="96.0" text="Creation Date" />
                            <TableColumn fx:id="XColumn" prefWidth="30.0" text="X" />
                            <TableColumn fx:id="YColumn" prefWidth="48.0" text="Y" />
                            <TableColumn fx:id="minimalPointColumn" prefWidth="163.0" text="Minimal Point" />
                            <TableColumn fx:id="tunedInWorksColumn" prefWidth="154.0" text="Tuned In Works" />
                            <TableColumn fx:id="averagePointColumn" prefWidth="29.0" text="Average Point" />
                            <TableColumn fx:id="difficultyColumn" prefWidth="111.0" text="Difficulty" />
                            <TableColumn fx:id="disciplineNameColumn" prefWidth="75.0" text="Discipline Name" />
                            <TableColumn fx:id="labsCountColumn" prefWidth="75.0" text="Labs Count" />
                        </columns>
                        <columnResizePolicy>
                            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                        </columnResizePolicy>
                    </TableView>
                </AnchorPane>
            </Tab>
            <Tab fx:id="canvasTab" text="CanvasTab">
                <AnchorPane fx:id="canvasPane" prefHeight="180.0" prefWidth="200.0" style="-fx-background-color: white,
                                 linear-gradient(from 0.5px 0.0px to 10.5px  0.0px, repeat, grey 5%, transparent 5%),                             linear-gradient(from 0.0px 0.5px to  0.0px 10.5px, repeat, grey 5%, transparent 5%);">
                    <cursor>
                        <Cursor fx:constant="CROSSHAIR" />
                    </cursor>
                </AnchorPane>
            </Tab>
        </TabPane>
        <Separator prefHeight="0.0" prefWidth="1100.0" />
        <FlowPane alignment="CENTER" columnHalignment="CENTER" hgap="10.0" prefHeight="86.0" prefWidth="898.0" vgap="10.0" VBox.vgrow="SOMETIMES">
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </VBox.margin>
            <Button fx:id="infoButton" mnemonicParsing="false" onAction="#infoButtonOnAction" text="Info">
                <tooltip>
                    <Tooltip fx:id="infoButtonTooltip" text="InfoButtonTooltip" />
                </tooltip>
            </Button>
            <Button fx:id="addButton" mnemonicParsing="false" onAction="#addButtonOnAction" text="Add">
                <tooltip>
                    <Tooltip fx:id="addButtonTooltip" text="AddButtonTooltip" />
                </tooltip>
            </Button>
            <Button fx:id="updateButton" mnemonicParsing="false" onAction="#updateButtonOnAction" text="Update">
                <tooltip>
                    <Tooltip fx:id="updateButtonTooltip" text="UpdateButtonTooltip" />
                </tooltip>
            </Button>
            <Button fx:id="removeButton" mnemonicParsing="false" onAction="#removeButtonOnAction" text="Remove">
                <tooltip>
                    <Tooltip fx:id="removeButtonTooltip" text="RemoveButtonTooltip" />
                </tooltip>
            </Button>
            <Button fx:id="removeGreaterButton" mnemonicParsing="false" onAction="#removeGreaterButtonOnAction" text="RemoveGreater">
                <tooltip>
                    <Tooltip fx:id="removeGreaterButtonTooltip" text="RemoveGreaterButtonTooltip" />
                </tooltip>
            </Button>
            <Button fx:id="clearButton" mnemonicParsing="false" onAction="#clearButtonOnAction" text="Clear">
                <tooltip>
                    <Tooltip fx:id="clearButtonTooltip" text="ClearButtonTooltip" />
                </tooltip>
            </Button>
            <Button fx:id="executeScriptButton" mnemonicParsing="false" onAction="#executeScriptButtonOnAction" text="ExecuteScript">
                <tooltip>
                    <Tooltip fx:id="executeScriptButtonTooltip" text="ExecuteScriptButtonTooltip" />
                </tooltip>
            </Button>
            <Button fx:id="addIfMinButton" mnemonicParsing="false" onAction="#addIfMinButtonOnAction" text="AddIfMin">
                <tooltip>
                    <Tooltip fx:id="addIfMinButtonTooltip" text="AddIfMinButtonTooltip" />
                </tooltip>
            </Button>
            <Button fx:id="filterButton" mnemonicParsing="false" onAction="#filterButtonOnAction" text="FilterStartWithName">
                <tooltip>
                    <Tooltip fx:id="filterButtonTooltip" text="FilterButtonTooltip" />
                </tooltip>
            </Button>
            <Button fx:id="sumMinimalPointButton" mnemonicParsing="false" onAction="#sumMinimalPointOnAction" text="SumMinimalPoint">
                <tooltip>
                    <Tooltip fx:id="sumMinimalPointButtonTooltip" text="SumMinimalPointButtonTooltip" />
                </tooltip>
            </Button>
            <Button fx:id="countLessThanButton" mnemonicParsing="false" onAction="#countLessThanOnAction" text="CountLessThan">
                <tooltip>
                    <Tooltip fx:id="countLessThanButtonTooltip" text="CountLessThanButtonTooltip" />
                </tooltip>
            </Button>
        </FlowPane>
    </VBox>
    <HBox alignment="CENTER_RIGHT" spacing="10.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0">
        <Label fx:id="usernameLabel" text="Username" />
        <Button fx:id="refreshButton" mnemonicParsing="false" onAction="#refreshButtonOnAction" text="Refresh">
            <tooltip>
                <Tooltip fx:id="refreshButtonTooltip" text="RefreshButtonTooltip" />
            </tooltip>
        </Button>
        <ComboBox fx:id="languageComboBox" prefWidth="150.0" promptText="Language" />
    </HBox>
</AnchorPane>
